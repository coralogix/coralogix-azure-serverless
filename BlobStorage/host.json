{
  "version": "2.0",
  "aggregator": {
    "batchSize": 1,
    "flushTimeout": "00:00:30"
  },
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 3, 
  },
  "extensions": {
    "blobs": {
      "maxDegreeOfParallelism": 1
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  }
}
